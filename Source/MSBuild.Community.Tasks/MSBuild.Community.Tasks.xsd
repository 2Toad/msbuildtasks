	<xs:element name="AssemblyInfo" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="CodeLanguage" use="required" />
					<xs:attribute name="ComVisible" type="xs:boolean" />
					<xs:attribute name="CLSCompliant" type="xs:boolean" />
					<xs:attribute name="Guid" />
					<xs:attribute name="AssemblyTitle" />
					<xs:attribute name="AssemblyDescription" />
					<xs:attribute name="AssemblyConfiguration" />
					<xs:attribute name="AssemblyCompany" />
					<xs:attribute name="AssemblyProduct" />
					<xs:attribute name="AssemblyCopyright" />
					<xs:attribute name="AssemblyTrademark" />
					<xs:attribute name="AssemblyCulture" />
					<xs:attribute name="AssemblyVersion" />
					<xs:attribute name="AssemblyFileVersion" />
					<xs:attribute name="OutputFile" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FileUpdate" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="Files" />
					<xs:attribute name="IgnoreCase" type="xs:boolean" />
					<xs:attribute name="Multiline" type="xs:boolean" />
					<xs:attribute name="Regex" type="xs:string" />
					<xs:attribute name="ReplacementCount" type="xs:int" />
					<xs:attribute name="ReplacementText" type="xs:string" />
					<xs:attribute name="Singleline" type="xs:boolean" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FtpUpload" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="LocalFile" type="xs:string" use="required" />
					<xs:attribute name="Password" type="xs:string" />
					<xs:attribute name="RemoteUri" type="xs:string" use="required" />
					<xs:attribute name="UsePassive" type="xs:boolean" />
					<xs:attribute name="Username" type="xs:string" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="ILMergeTargetKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Dll" />
			<xs:enumeration value="Exe" />
			<xs:enumeration value="WinExe" />
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="ILMerge" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:ToolTaskType">
					<xs:attribute name="AllowDuplicateTypes" />
					<xs:attribute name="AllowZeroPeKind" type="xs:boolean" />
					<xs:attribute name="AttributeFile" />
					<xs:attribute name="Closed" type="xs:boolean" />
					<xs:attribute name="CopyAttributes" type="xs:boolean" />
					<xs:attribute name="DebugInfo" type="xs:boolean" />
					<xs:attribute name="DelaySign" type="xs:boolean" />
					<xs:attribute name="ExcludeFile" />
					<xs:attribute name="InputAssemblies" use="required" />
					<xs:attribute name="KeyFile" />
					<xs:attribute name="LogFile" />
					<xs:attribute name="OutputFile" use="required" />
					<xs:attribute name="PublicKeyTokens" type="xs:boolean" />
					<xs:attribute name="TargetPlatformVersion" />
					<xs:attribute name="TargetPlatformDirectory" />
					<xs:attribute name="TargetKind" type="msb:ILMergeTargetKind" />
					<xs:attribute name="Version" />
					<xs:attribute name="XmlDocumentation" type="xs:boolean" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NDoc" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:ToolTaskType">
					<xs:attribute name="Documenter" use="required" />
					<xs:attribute name="ProjectFilePath" use="required" />
					<xs:attribute name="Verbose" type="xs:boolean" />
					<xs:attribute name="WorkingDirectory" />
					<xs:attribute name="ToolPath" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="NUnit" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:ToolTaskType">
					<xs:attribute name="Assemblies" use="required" />
					<xs:attribute name="IncludeCategory" />
					<xs:attribute name="ExcludeCategory" />
					<xs:attribute name="Fixture" />
					<xs:attribute name="XsltTransformFile" />
					<xs:attribute name="OutputXmlFile" />
					<xs:attribute name="WorkingDirectory" />
					<xs:attribute name="ToolPath" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SvnClientType">
		<xs:complexContent>
			<xs:extension base="msb:ToolTaskType">
				<xs:attribute name="Command" />
				<xs:attribute name="Arguments" />
				<xs:attribute name="Username" />
				<xs:attribute name="Password" />
				<xs:attribute name="Verbose" type="xs:boolean" />
				<xs:attribute name="Force" type="xs:boolean" />
				<xs:attribute name="Message" />
				<xs:attribute name="RepositoryPath" />
				<xs:attribute name="LocalPath" />
				<xs:attribute name="Targets" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="SvnCheckout" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:SvnClientType">
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvnClient" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:SvnClientType">
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvnCommit" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:SvnClientType">
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvnExport" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:SvnClientType">
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvnUpdate" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:SvnClientType">
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SvnVersion" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:ToolTaskType">
					<xs:attribute name="LocalPath" use="required" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="DateTimeKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Local" />
			<xs:enumeration value="Utc" />
			<xs:enumeration value="Unspecified" />
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Time" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="Format" />
					<xs:attribute name="Kind" type="msb:DateTimeKind" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Version" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="VersionFile" use="required" />
					<xs:attribute name="BuildType" />
					<xs:attribute name="RevisionType" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Xslt" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="Inputs" use="required" />
					<xs:attribute name="RootTag" />
					<xs:attribute name="RootAttributes" />
					<xs:attribute name="Xsl" use="required" />
					<xs:attribute name="Output" use="required" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Zip" substitutionGroup="msb:Task">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="msb:TaskType">
					<xs:attribute name="ZipFileName" use="required" />
					<xs:attribute name="ZipLevel" type="xs:int" />
					<xs:attribute name="Files" use="required" />
					<xs:attribute name="Flatten" type="xs:boolean" />
					<xs:attribute name="Comment" />
					<xs:attribute name="WorkingDirectory" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
